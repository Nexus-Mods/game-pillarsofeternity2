    "@types/react-i18next": "^7.6.1",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/semver": "^5.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "bluebird": "^3.5.1",
    "copyfiles": "^2.0.0",
    "react-dnd": "^2.5.4",
